{
	"name": "@svar-ui/svelte-filter",
	"version": "2.3.0",
	"description": "SVAR Svelte Filter - a flexible and customizable filter builder (query builder) for Svelte apps",
	"productTag": "filter",
	"productTrial": false,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --host 0.0.0.0  --port 5100 --mode test",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js",
			"types": "./types/index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/svar-widgets/filter.git"
	},
	"bugs": {
		"url": "https://forum.svar.dev"
	},
	"homepage": "https://svar.dev/svelte/filter/",
	"dependencies": {
		"@svar-ui/lib-dom": "0.9.2",
		"@svar-ui/lib-state": "1.9.6",
		"@svar-ui/filter-locales": "2.3.0",
		"@svar-ui/filter-store": "2.3.0",
		"@svar-ui/svelte-core": "2.3.1",
		"@svar-ui/svelte-menu": "2.3.0"
	},
	"files": [
		"src",
		"types",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"ui component",
		"filter",
		"filter builder",
		"query",
		"query builder",
		"filtering",
		"svelte5",
		"svar widgets"
	]
}
